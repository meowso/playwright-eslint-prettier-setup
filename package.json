{
  "name": "@meowsos/playwright-setup",
  "version": "0.0.12",
  "description": "Modern Eslint and Prettier setup for Playwright Typescript projects",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meowso/playwright-hero.git"
  },
  "keywords": [ "playwright", "typescript", "eslint", "eslint-config", "prettier", "eslint-config-prettier", "eslint-plugin-prettier", "eslint-plugin-unicorn", "eslint-plugin-sonarjs", "eslint-plugin-playwright"],
  "scripts": {
    "peerdeps": "npx install-peerdeps --dev @meowsos/eslint-config && npx install-peerdeps --dev @meowsos/prettier-config",
    "publish-eslint-config": "npm publish --workspace @meowsos/eslint-config",
    "publish-prettier-config": "npm publish --workspace @meowsos/prettier-config"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Meowso <m3au@pm.me> (http://about.me/bpalma)",
  "contributors": [
    {
      "name": "Manonmani Ramu",
      "email": "mananmani.ramu@gmail.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meowso/playwright-hero/issues"
  },
  "homepage": "https://github.com/meowso/playwright-hero#readme",
  "dependencies": {
    "@changesets/cli": "^2.27.1"
  },
  "engines": {
    "node": ">=20.9.0",
    "yarn": ">=1.22.10"
  }
}
