{
  "name": "@meowsos/playwright-setup",
  "version": "0.0.18",
  "description": "Modern Eslint and Prettier setup for Playwright Typescript projects",
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meowso/playwright-hero.git"
  },
  "keywords": [
    "playwright",
    "typescript",
    "eslint",
    "eslint-config",
    "prettier",
    "eslint-config-prettier",
    "eslint-plugin-prettier",
    "eslint-plugin-unicorn",
    "eslint-plugin-sonarjs",
    "eslint-plugin-playwright"
  ],
  "scripts": {
    "peerdeps": "npx install-peerdeps @meowsos/eslint-config && npx install-peerdeps @meowsos/prettier-config",
    "publish-eslint-config": "npm publish --workspace @meowsos/eslint-config",
    "publish-prettier-config": "npm publish --workspace @meowsos/prettier-config"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@meowsos/eslint-config": "^1.1.1",
    "@meowsos/prettier-config": "^1.1.1"
  },
  "engines": {
    "node": ">=20.9.0",
    "yarn": ">=1.22.10"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Meowso <m3au@pm.me> (http://about.me/bpalma)",
  "contributors": [
    {
      "name": "Manonmani Ramu",
      "email": "mananmani.ramu@gmail.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meowso/playwright-hero/issues"
  },
  "homepage": "https://github.com/meowso/playwright-hero#readme",
  "peerDependencies": {
    "@playwright/test": "1.40",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-playwright": "^0.20.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
